{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "master"
  },
  "defaultProject": "frontend",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/build",
        "{projectRoot}/lib",
        "{projectRoot}/.vercel/output"
      ],
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**",
        "{projectRoot}/package.json",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/tsconfig.*.json",
        "{projectRoot}/vite.config.ts",
        "{projectRoot}/index.html",
        "{workspaceRoot}/tsconfig.base.json",
        "{workspaceRoot}/vercel.json",
        "{workspaceRoot}/package-lock.json"
      ]
    },
    "test": {
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**",
        "{projectRoot}/**/*.test.ts",
        "{projectRoot}/**/*.test.tsx",
        "{projectRoot}/vitest.config.ts",
        "{workspaceRoot}/package-lock.json"
      ]
    },
    "test:coverage": {
      "cache": true,
      "outputs": ["{projectRoot}/coverage"],
      "inputs": [
        "{projectRoot}/src/**",
        "{projectRoot}/**/*.test.ts",
        "{projectRoot}/**/*.test.tsx",
        "{projectRoot}/vitest.config.ts",
        "{workspaceRoot}/package-lock.json"
      ]
    },
    "lint": {
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**",
        "{projectRoot}/.eslintrc.*",
        "{projectRoot}/eslint.config.js",
        "{projectRoot}/package.json",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/eslint.config.js"
      ]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/tsconfig.*.json",
        "{workspaceRoot}/tsconfig.base.json"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "emulators": {
      "cache": false,
      "persistent": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": [
      "!{projectRoot}/**/*.test.ts",
      "!{projectRoot}/**/*.test.tsx",
      "!{projectRoot}/**/*.spec.ts",
      "!{projectRoot}/**/*.spec.tsx",
      "!{projectRoot}/vitest.config.ts",
      "!{projectRoot}/.eslintrc.*",
      "!{projectRoot}/eslint.config.js"
    ],
    "firebase": [
      "{workspaceRoot}/firebase.json",
      "{workspaceRoot}/firestore.rules",
      "{workspaceRoot}/firestore.indexes.json",
      "{workspaceRoot}/storage.rules"
    ],
    "vercel": ["{workspaceRoot}/vercel.json", "{projectRoot}/.vercel/**"],
    "sharedGlobals": [
      "{workspaceRoot}/package-lock.json",
      "{workspaceRoot}/tsconfig.base.json"
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "test:coverage",
          "lint",
          "typecheck"
        ],
        "parallel": 3,
        "useDaemonProcess": false
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "parallel": 3,
  "cacheDirectory": ".nx/cache",
  "useDaemonProcess": false
}
