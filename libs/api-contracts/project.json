{
  "name": "api-contracts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-contracts/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/api-contracts/dist",
        "tsConfig": "libs/api-contracts/tsconfig.json",
        "main": "libs/api-contracts/src/index.ts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p libs/api-contracts/tsconfig.json"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "libs/api-contracts/vitest.config.ts"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf libs/api-contracts/dist"
      }
    }
  },
  "tags": ["type:library", "scope:shared"]
}
